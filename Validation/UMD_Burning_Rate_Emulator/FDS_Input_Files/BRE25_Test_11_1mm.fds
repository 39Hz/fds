! Reference: A. Markan, P. B. Sunderland, J. G. Quintiere, J. L. de Ris, D. P. Stocker, H. R. Baum.
! A Burning Rate Emulator (BRE) for study of condensed fuel burning in microgravity. Comb. and Flame,
! 192:272-282, 2018.
!
! See Markan et al. 2018, Table 3, for a key to the Test No. series.
!
! Stoichiometric mixture fraction for pure C2H4 is Z_stoich = 0.0876.
! Stoichiometric mixture fraction for 50% C2H4/50% N2 is Z_stoich = 0.1610.

&HEAD CHID='BRE25_Test_11_1mm', TITLE='UMD Burning Rate Emulator, Markan et al. 2018'/

&MESH IJK=80,1,120, XB=0,0.080,-0.0005,0.0005,-0.04,0.080, CYLINDRICAL=T/

&TIME T_END=5./

&DUMP DT_SLCF=0.1, DT_DEVC=0.1, DT_DEVC_LINE=0.1/

&MISC SIMULATION_MODE='DNS', GVEC(3)=0., P_INF=82073.3, STRATIFICATION=F
      Y_O2_INFTY=0.2864, Y_CO2_INFTY=0., HUMIDITY=0., TMPA=25, CHECK_HT=F/

&COMB RADIATIVE_FRACTION=0.0, SUPPRESSION=F /

&RADI PATH_LENGTH=0.01 /

&REAC FUEL='50% C2H4'/

&SPEC ID='50% C2H4',
      VOLUME_FRACTION(1)=0.5, SPEC_ID(1)='ETHYLENE',
      VOLUME_FRACTION(2)=0.5, SPEC_ID(2)='NITROGEN'/

&SPEC ID='ETHYLENE', LUMPED_COMPONENT_ONLY=T/
&SPEC ID='NITROGEN', LUMPED_COMPONENT_ONLY=T/

&SURF ID='burner', TAU_MF=0.01, MASS_FLUX=7.5E-03, SPEC_ID='50% C2H4', TMP_FRONT=32.9, COLOR='RED'/

&OBST XB=0,0.0125,-0.0005,0.0005,-0.04,0, SURF_IDS='burner','INERT','INERT'/

&VENT PBZ=-0.040, SURF_ID='OPEN'/
&VENT PBZ=0.080, SURF_ID='OPEN'/
&VENT PBX=0.080, SURF_ID='OPEN'/

&SLCF PBY=0, QUANTITY='VELOCITY', VECTOR=T/
&SLCF PBY=0, QUANTITY='TEMPERATURE', CELL_CENTERED=T/
&SLCF PBY=0, QUANTITY='MIXTURE FRACTION', CELL_CENTERED=T/

&DEVC XB=0.001,0.001,0,0,0,0.080, POINTS=80, QUANTITY='MIXTURE FRACTION', ID='Z'/
&DEVC XB=0.001,0.001,0,0,0,0.080, POINTS=80, QUANTITY='TEMPERATURE', ID='T', HIDE_COORDINATES=T/

&DEVC XYZ=0.00000,0,0.0001, IOR=3, QUANTITY='CONVECTIVE HEAT FLUX', ID='qconv0mm'/
&DEVC XYZ=0.00825,0,0.0001, IOR=3, QUANTITY='CONVECTIVE HEAT FLUX', ID='qconv8mm'/

&DEVC XYZ=0.00000,0,0.0001, IOR=3, QUANTITY='RADIATIVE HEAT FLUX', ID='qrad0mm'/
&DEVC XYZ=0.00825,0,0.0001, IOR=3, QUANTITY='RADIATIVE HEAT FLUX', ID='qrad8mm'/

&DEVC XYZ=0.00000,0,0.0001, IOR=3, QUANTITY='NET HEAT FLUX', ID='qnet0mm'/
&DEVC XYZ=0.00825,0,0.0001, IOR=3, QUANTITY='NET HEAT FLUX', ID='qnet8mm'/

&TAIL /
